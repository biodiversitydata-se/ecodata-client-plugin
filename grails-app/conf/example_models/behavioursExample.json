{
  "modelName": "Behaviours example",
  "dataModel": [
    {
      "dataType": "number",
      "name": "item1"
    },
    {
      "dataType": "number",
      "name": "item2",
      "behaviour": [
        {
          "condition": "item1 > 3",
          "type": "enable"
        }
      ]
    },
    {
      "dataType": "list",
      "name": "nested",
      "columns": [
        {
          "dataType": "number",
          "name": "item3"
        },
        {
          "dataType": "number",
          "name": "item4",
          "computed": {
            "expression": "item3*item2"
          }
        }
      ]
    }
  ],
  "viewModel": [
    {
      "type": "row",
      "items": [
        {
          "type": "col",
          "items": [
            {
              "preLabel": "Item 1",
              "source": "item1",
              "type": "number"
            },
            {
              "preLabel": "Item 2",
              "source": "item2",
              "type": "number"
            }
          ]
        }
      ]
    },
    {
      "type": "table",
      "source": "nested",
      "columns": [
        {
          "type": "number",
          "source": "item3",
          "title": "Item 3"
        },
        {
          "type": "number",
          "source": "item4",
          "title": "Item 4"
        }
      ]

    }
  ],
  "title": "Behaviours example"
}